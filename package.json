{
  "name": "cloudcanvas",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "compile": "turbo run compile",
    "installDeps": "turbo run installDeps",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "projen": "turbo run projen",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@ggallon/prettier-plugin-sort-imports": "^3.4.2-canary.0",
    "@swc-node/jest": "^1.4.3",
    "@tldraw/lfg": "latest",
    "@types/node": "^17.0.14",
    "prettier": "^2.7.1",
    "turbo": "^1.3.4",
    "typescript": "^4.7.3",
    "webpack": "^5.68.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
    "minimist": "1.2.6",
    "path-browserify": "^1.0.1",
    "ts-loader": "^9.4.1"
  },
  "packageManager": "yarn@1.22.10"
}
